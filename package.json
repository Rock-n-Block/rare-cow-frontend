    "axios": "^0.26.0",
    "bignumber.js": "^9.0.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "ethereum-types": "^3.2.0",
    "formik": "^2.2.9",
    "history": "^4.10.0",
    "moment": "^2.29.3",
    "node-sass": "^6.0.0",
    "rc-dialog": "^8.7.0",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "17.0.2",
    "react-dropzone": "^12.0.4",
    "react-loading-skeleton": "^3.1.0",
    "react-outside-click-handler": "^1.3.0",
    "react-paginate": "^8.1.2",
    "react-redux": "^7.2.1",
