{
  "name": "chimer-kingdome",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "new": "plop --plopfile generators/index.js",
    "lint": "eslint --cache",
    "test:update": "jest --updateSnapshot ./src",
    "test:watch": "jest ./src",
    "test:coverage": "jest --coverage --watchAll=false ./src",
    "generate-types": "node ./scripts/generate-types.js",
    "download-schema": "node ./scripts/download-schema.js",
    "storybook": "start-storybook ./public -p 6006",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "bash -c tsc"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@amfi/connect-wallet": "^1.1.24",
